{
  "name": "cms-cache",
  "version": "0.1.0",
  "description": "Agnostic in-memory cache layer for any CMS provider.",
  "license": "MIT",
  "author": "Mestre",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "eslint src test --ext .ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "tsup": "^8.1.0",
    "vitest": "^2.0.0",
    "rimraf": "^6.0.0",
    "eslint": "^9.0.0"
  }
}
